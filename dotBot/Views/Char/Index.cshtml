@{
}
<form method="post">
	<input type="text" name="Id" value="346320821" />
<label>&nbsp;</label>
			<input type="submit" value="вывести" class="submit" />
</form>
<h1>
	@ViewBag.userName
</h1>
@if (ViewBag.HaveData)
{
	<table class="iksweb">
		<tbody>
			<tr>
				<td>Уровень:</td>
				<td>@ViewBag.userLvl</td>
			</tr>
			<tr>
				<td>Опыт:</td>
				<td>@ViewBag.userExp / @ViewBag.userExpToUp</td>
			</tr>
			<tr>
				<td>Здоровье:</td>
				<td>@ViewBag.userHp / @ViewBag.userHpMax</td>
			</tr>
			<tr>
				<td>Сила:</td>
				<td>@ViewBag.userpower</td>
			</tr>
			<tr>
				<td>Защита:</td>
				<td>@ViewBag.userDefence</td>
			</tr>
			<tr>
				<td>Очки уровней:</td>
				<td>@ViewBag.userLvlPoints</td>
			</tr>
			<tr>
				<td>GOLD:</td>
				<td>@ViewBag.userMoney</td>
			</tr>
		</tbody>
	</table>

}
